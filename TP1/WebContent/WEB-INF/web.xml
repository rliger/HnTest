<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
	<servlet>
		<servlet-name>CreationClient</servlet-name>
		<servlet-class>fr.tp1.servlets.CreationClient</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreationCommande</servlet-name>
		<servlet-class>fr.tp1.servlets.CreationCommande</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>CreationClient</servlet-name>
		<url-pattern>/creationclient</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CreationCommande</servlet-name>
		<url-pattern>/creationcommande</url-pattern>
	</servlet-mapping>

</web-app>